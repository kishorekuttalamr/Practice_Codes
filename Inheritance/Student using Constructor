#include <iostream>
using namespace std;
class Student
{
public:
    string name;
    int roll_no;
    Student()
    {
        name = "John";
        roll_no = 2;
    }
    void display()
    {
        cout << name << endl;
        cout << roll_no << endl;
    }
};
int main()
{
    Student s;
    s.display();
}
